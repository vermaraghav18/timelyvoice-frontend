{
  "version": 2,
  "functions": {
    "api/edge-ssr/article/[slug].ts": {
      "runtime": "edge"
    }
  },
  "routes": [
  
    {
      "src": "^/article/(.*)$",
      "dest": "/api/edge-ssr/article/$1"
    },

   
    {
      "src": "^/api/(.*)",
      "dest": "https://timelyvoice-backend.onrender.com/api/$1"
    },
    {
      "src": "^/uploads/(.*)",
      "dest": "https://timelyvoice-backend.onrender.com/uploads/$1"
    },
    {
      "src": "^/sitemap.xml",
      "dest": "https://timelyvoice-backend.onrender.com/sitemap.xml"
    },
    {
      "src": "^/news-sitemap.xml",
      "dest": "https://timelyvoice-backend.onrender.com/news-sitemap.xml"
    },
    {
      "src": "^/robots.txt",
      "dest": "https://timelyvoice-backend.onrender.com/robots.txt"
    },
    {
      "src": "^/rss.xml",
      "dest": "https://timelyvoice-backend.onrender.com/rss.xml"
    },
    {
      "src": "^/rss/(.*)",
      "dest": "https://timelyvoice-backend.onrender.com/rss/$1"
    },


    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
